{
  "name": "kws3/api-core",
  "version": "2.1.1",
  "description": "KWS3 Api Core",
  "license": "MIT",
  "scripts": {
    "test": "php bin/apicore Test",
    "lint:fix": "phpcbf -p -s -v -n ./ --standard=ruleset.xml --extensions=php"
  },
  "authors": [
    {
      "name": "ekhaled",
      "email": "ekhaled983@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Kws3\\ApiCore\\": "src/"
    }
  },
  "require": {
    "bcosca/fatfree-core": "3.7.3",
    "ikkez/f3-cortex": "1.7.5",
    "itsgoingd/clockwork": "4.1.8",
    "aws/aws-sdk-php": "^3.18",
    "ekhaled/schema-parser-mysql": "1.3",
    "ekhaled/f3-cortex-model-generator": "1.8",
    "davedevelopment/phpmig": "1.5",
    "pimple/pimple": "~3.0",
    "phpmailer/phpmailer": "^5.2",
    "intervention/image": "^2.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6.2",
    "slevomat/coding-standard": "^7.0.18"
  },
  "bin": [
    "bin/apicore"
  ],
  "config": {
    "platform":{"php": "7.4"},
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
